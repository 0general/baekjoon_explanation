   "source": [
    "n = int(input())\n",
    "\n",
    "dp = [1]*(n+1)\n",
    "if n >= 2:\n",
    "    for i in range(2, n+1):\n",
    "        dp[i] = dp[i-1]+dp[i-2]\n",
    "print(dp[n] % 10007)"
   ]
  }
