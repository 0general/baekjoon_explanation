   "outputs": [],
   "source": [
    "n = int(input())\n",
    "dp = [0] + list(map(int, input().split()))\n",
    "\n",
    "for now in range(2, n+1):\n",
    "    for i in range(1, now//2 + 1):\n",
    "        dp[now] = min(dp[now], dp[i] + dp[now-i])\n",
    "\n",
    "print(dp[n])"
   ]
  }
